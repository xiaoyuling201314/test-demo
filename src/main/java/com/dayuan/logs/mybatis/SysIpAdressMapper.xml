<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dayuan.logs.mapper.SysIpAdressMapper">
    <resultMap id="baseResultMap" type="com.dayuan.logs.bean.SysIpAdress">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="system_ip" property="systemIp" jdbcType="VARCHAR"/>
        <result column="system_address" property="systemAddress" jdbcType="VARCHAR"/>
        <result column="delete_flag" property="deleteFlag" jdbcType="SMALLINT"/>
        <result column="create_date" property="createDate" jdbcType="TIMESTAMP"/>
        <result column="update_date" property="updateDate" jdbcType="TIMESTAMP"/>
        <result column="param1" property="param1" jdbcType="VARCHAR"/>
        <result column="param2" property="param2" jdbcType="VARCHAR"/>
        <result column="param3" property="param3" jdbcType="VARCHAR"/>
    </resultMap>

    <resultMap id="resultMapModel" type="com.dayuan.logs.model.SysIpAdressModel">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="system_ip" property="systemIp" jdbcType="VARCHAR"/>
        <result column="system_address" property="systemAddress" jdbcType="VARCHAR"/>
        <result column="delete_flag" property="deleteFlag" jdbcType="SMALLINT"/>
        <result column="create_date" property="createDate" jdbcType="TIMESTAMP"/>
        <result column="update_date" property="updateDate" jdbcType="TIMESTAMP"/>
        <result column="param1" property="param1" jdbcType="VARCHAR"/>
        <result column="param2" property="param2" jdbcType="VARCHAR"/>
        <result column="param3" property="param3" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="baseColumnList">
        id,system_ip, system_address, create_date, update_date, param1, param2, param3
    </sql>

    <select id="selectByPrimaryKey" resultMap="baseResultMap" parameterType="java.lang.Integer">
        SELECT
        <include refid="baseColumnList"/>
        FROM sys_ip_adress
        WHERE delete_flag = 0 AND id = #{id}
    </select>

    <update id="deleteByPrimaryKey">
        UPDATE sys_ip_adress
        SET
        delete_flag = 1,
        update_date = now()
        WHERE id IN
        <foreach collection="array" item="ids" index="index" open="(" close=")" separator=",">
            #{ids}
        </foreach>
    </update>

    <update id="deleteByPrimaryKeys" parameterType="java.lang.String">
        UPDATE sys_ip_adress SET delete_flag=1, update_date=now(), update_by = #{updateUserId}
        WHERE id IN
        <foreach collection="ids" item="id" index="index" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </update>

    <insert id="insert" parameterType="com.dayuan.logs.bean.SysIpAdress">
        INSERT INTO sys_ip_adress (system_ip, system_address, param1, param2, param3)
        VALUES (#{systemIp}, #{systemAddress}, #{param1}, #{param2}, #{param3})
    </insert>

    <insert id="insertSelective" parameterType="com.dayuan.logs.bean.SysIpAdress">
        INSERT INTO sys_ip_adress
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="systemIp != null">
                system_ip,
            </if>
            <if test="systemAddress != null">
                system_address,
            </if>
            <if test="param1 != null">
                param1,
            </if>
            <if test="param2 != null">
                param2,
            </if>
            <if test="param3 != null">
                param3,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="systemIp != null">
                #{systemIp},
            </if>
            <if test="systemAddress != null">
                #{systemAddress},
            </if>
            <if test="param1 != null">
                #{param1},
            </if>
            <if test="param2 != null">
                #{param2},
            </if>
            <if test="param3 != null">
                #{param3},
            </if>
        </trim>
    </insert>

    <update id="updateByPrimaryKeySelective" parameterType="com.dayuan.logs.bean.SysIpAdress">
        UPDATE sys_ip_adress
        <set>
            <if test="systemIp != null">
                 system_ip = #{systemIp},
            </if>
            <if test="systemAddress != null">
                 system_address = #{systemAddress},
            </if>
            <if test="param1 != null">
                 param1 = #{param1},
            </if>
            <if test="param2 != null">
                 param2 = #{param2},
            </if>
            <if test="param3 != null">
                 param3 = #{param3},
            </if>
        </set>
        WHERE id = #{id}
    </update>

    <update id="updateByPrimaryKey" parameterType="com.dayuan.logs.bean.SysIpAdress">
        UPDATE sys_ip_adress
        SET
            system_ip = #{systemIp},
            system_address = #{systemAddress},
            param1 = #{param1},
            param2 = #{param2},
            param3 = #{param3}
        WHERE id = #{id}
    </update>

    <!-- 分页基础查询 -->
    <select id="loadDatagrid" resultMap="resultMapModel" parameterType="Object">
        SELECT *
        FROM sys_ip_adress
        WHERE delete_flag = 0
        <if test="obj.keyWords != null and obj.keyWords != ''">
            AND
            (
            id LIKE CONCAT('%',#{obj.keyWords},'%')
            OR
            id LIKE CONCAT('%',#{obj.keyWords},'%')
            )
        </if>
        LIMIT #{rowOffset} , #{pageSize}
    </select>

    <select id="getRowTotal" resultType="java.lang.Integer" parameterType="Object">
        SELECT count(1) AS rowTotal
        FROM sys_ip_adress
        WHERE delete_flag = 0
        <if test="obj.keyWords != null and obj.keyWords != ''">
            AND
            (
            id LIKE CONCAT('%',#{obj.keyWords},'%')
            OR
            id LIKE CONCAT('%',#{obj.keyWords},'%')
            )
        </if>
    </select>
    <select id="querByIp" resultMap="baseResultMap" parameterType="java.lang.String">
        SELECT
        <include refid="baseColumnList"/>
        FROM sys_ip_adress
        WHERE delete_flag = 0 AND system_ip = #{ip}
    </select>
</mapper>