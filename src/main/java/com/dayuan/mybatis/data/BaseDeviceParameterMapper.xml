   <!-- 查询条件 tb1.device_type_id='26426d3c89314586bfb0c47cfb574cd4' and tb1.delete_flag=0-->
	<sql id="Example_Where_Clause">
		where tb1.delete_flag = 0 and tb2.delete_flag = 0
		<if test="obj.baseBean != null">
			<if test="obj.baseBean.deviceTypeId != null and obj.baseBean.deviceTypeId != ''">
				and tb1.device_type_id = #{obj.baseBean.deviceTypeId}
			</if>
			<if test="obj.baseBean.itemId != null and obj.baseBean.itemId != ''">
              and (tb2.detect_item_name like CONCAT('%', #{obj.baseBean.itemId}, '%')
              or tb1.project_type like CONCAT('%', #{obj.baseBean.itemId}, '%')
              or tb1.detect_method like CONCAT('%', #{obj.baseBean.itemId}, '%')
              )
            </if>
		</if>
	</sql>
  <select id="selectByPrimaryKey" resultMap="joinBaseResultMap" parameterType="java.lang.String" >
	    select 
	    tb1.*,tb2.detect_item_name as itemName
	    from base_device_parameter tb1
		LEFT JOIN base_detect_item tb2 on tb1.item_id=tb2.id
	    where tb1.id = #{id,jdbcType=VARCHAR}
  </select>
