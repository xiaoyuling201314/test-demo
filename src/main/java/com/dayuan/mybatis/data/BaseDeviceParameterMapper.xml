  <select id="queryByUniqueDeviceItem" resultMap="BaseResultMap" parameterType="com.dayuan.bean.data.BaseDeviceParameter">
    select 
    <include refid="Base_Column_List" />
    from base_device_parameter 
    where delete_flag=0 and device_type_id = #{deviceTypeId,jdbcType=VARCHAR} and item_id=#{itemId,jdbcType=VARCHAR}
    and project_type=#{projectType,jdbcType=VARCHAR}
    and detect_method=#{detectMethod,jdbcType=VARCHAR}
    <if test="reserved3!=null and reserved3!=''">
      and  reserved3 = #{reserved3,jdbcType=VARCHAR}
    </if>
  </select>
  
  <select id="queryByDeviceItem" resultMap="BaseResultMap" parameterType="com.dayuan.bean.data.BaseDeviceParameter">
    select 
    <include refid="Base_Column_List" />
    from base_device_parameter 
    where delete_flag = 0 
    <if test="deviceTypeId != null and deviceTypeId != ''">
    	and device_type_id = #{deviceTypeId,jdbcType=VARCHAR} 
    </if>
    <if test="itemId != null and itemId != ''">
    	and item_id=#{itemId,jdbcType=VARCHAR} 
    </if>
  </select>
  
  <select id="queryByItemG" resultMap="BaseResultMap" parameterType="java.lang.String">
    select 
    <include refid="Base_Column_List" />
    from base_device_parameter 
    where delete_flag = 0 
    <if test="itemId != null and itemId != ''">
    	and item_id=#{itemId,jdbcType=VARCHAR} 
    </if>
    group by project_type,detect_method order by project_type asc
  </select>
  
  <select id="queryByDeviceTypeId" resultMap="BaseResultMap" parameterType="Object">
    select 
    <include refid="Base_Column_List" />
    from base_device_parameter 
    where device_type_id = #{deviceTypeId,jdbcType=VARCHAR} and delete_flag=0
  </select>

  <select id="queryListForExport" resultMap="exportResultMap" parameterType="Object">
    select (@i:=@i+1) seralNumber,
    tb1.*,tb2.detect_item_name itemName
    from base_device_parameter tb1
    INNER JOIN base_detect_item tb2 ON tb1.item_id=tb2.id
    ,(select @i:=0) as it
    where tb1.delete_flag = 0 and tb2.delete_flag = 0
    <if test="obj.baseBean != null">
      <if test="obj.baseBean.deviceTypeId != null and obj.baseBean.deviceTypeId != ''">
        and tb1.device_type_id = #{obj.baseBean.deviceTypeId}
      </if>
      <if test="obj.baseBean.itemName != null and obj.baseBean.itemName != ''">
        and tb2.detect_item_name like CONCAT('%', #{obj.baseBean.itemName}, '%')
      </if>
    </if>
    <if test="ids != null">
      AND tb1.id IN
      <foreach collection="ids" item="id" index="index" open="(" close=")" separator=",">
        #{id}
      </foreach>
    </if>
  </select>

